{
  "table": "UserTokens",
  "database": "application",
  "attributes": {
    "id": {
      "type": "INTEGER",
      "primaryKey": true,
      "autoIncrement": true
    },
    "token": {
      "type": "FLOAT",
      "defaultValue": 0
    },
    "amount": {
      "type": "STRING",
      "defaultValue": ""
    },
    "status": {
      "type": "STRING",
      "defaultValue": "add"
    },
    "_status": {
      "type": "STRING"
    },
    "isActive": {
      "type": "BOOLEAN",
      "defaultValue": true
    },
    "deletedAt": {
      "type": "DATE",
      "allowNull": true
    },
    "paymentDate": {
      "type": "DATE",
      "defaultValue": ""
    }
  },
  "associations": [
    {
      "data": [
        {
          "data": {
            "as": "Owner",
            "sourceKey": "id",
            "foreignKey": "createdBy"
          },
          "type": "belongsTo"
        },
        {
          "data": {
            "as": "Updater",
            "sourceKey": "id",
            "foreignKey": "updatedBy"
          },
          "type": "belongsTo"
        },
        {
          "data": {
            "as": "Destroyer",
            "sourceKey": "id",
            "foreignKey": "deletedBy"
          },
          "type": "belongsTo"
        },
        {
          "data": {
            "as": "User",
            "sourceKey": "id",
            "foreignKey": "userId"
          },
          "type": "belongsTo"
        }
      ],
      "model": "Users"
    }
  ]
}