{
  "table": "Permissions",
  "database": "application",
  "attributes": {
    "id": {
      "type": "INTEGER",
      "primaryKey": true,
      "autoIncrement": true
    },
    "icon": {
      "type": "STRING",
      "defaultValue": ""
    },
    "link": {
      "type": "STRING",
      "defaultValue": ""
    },
    "name": {
      "type": "STRING",
      "defaultValue": ""
    },
    "type": {
      "type": "STRING",
      "defaultValue": "menuitem"
    },
    "label": {
      "type": "STRING",
      "defaultValue": ""
    },
    "_status": {
      "type": "STRING"
    },
    "deletedAt": {
      "type": "DATE",
      "allowNull": true
    },
    "appComponent": {
      "type": "STRING",
      "defaultValue": ""
    }
  },
  "associations": [
    {
      "data": [
        {
          "data": {
            "sourceKey": "id",
            "foreignKey": "permissionId"
          },
          "type": "hasMany"
        }
      ],
      "model": "RolePermissions"
    },
    {
      "data": [
        {
          "data": {
            "sourceKey": "id",
            "foreignKey": "parentId"
          },
          "type": "belongsTo"
        }
      ],
      "model": "Permissions"
    },
    {
      "data": [
        {
          "data": {
            "as": "Owner",
            "sourceKey": "id",
            "foreignKey": "createdBy"
          },
          "type": "belongsTo"
        },
        {
          "data": {
            "as": "Updater",
            "sourceKey": "id",
            "foreignKey": "updatedBy"
          },
          "type": "belongsTo"
        },
        {
          "data": {
            "as": "Destroyer",
            "sourceKey": "id",
            "foreignKey": "deletedBy"
          },
          "type": "belongsTo"
        }
      ],
      "model": "Users"
    },
    {
      "data": [
        {
          "data": {
            "sourceKey": "id",
            "foreignKey": "permissionId"
          },
          "type": "hasMany"
        }
      ],
      "model": "UserPermissions"
    }
  ]
}