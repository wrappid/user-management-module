{
  "table": "PersonContacts",
  "database": "application",
  "attributes": {
    "id": {
      "type": "INTEGER",
      "primaryKey": true,
      "autoIncrement": true
    },
    "data": {
      "type": "STRING",
      "defaultValue": ""
    },
    "type": {
      "type": "STRING",
      "defaultValue": ""
    },
    "_status": {
      "type": "STRING"
    },
    "isActive": {
      "type": "BOOLEAN",
      "defaultValue": true
    },
    "verified": {
      "type": "BOOLEAN",
      "defaultValue": false
    },
    "deletedAt": {
      "type": "DATE",
      "allowNull": true
    },
    "primaryFlag": {
      "type": "BOOLEAN",
      "defaultValue": false
    },
    "notificationFlag": {
      "type": "BOOLEAN",
      "defaultValue": false
    }
  },
  "associations": [
    {
      "data": [
        {
          "data": {
            "as": "Owner",
            "sourceKey": "id",
            "foreignKey": "createdBy"
          },
          "type": "belongsTo"
        },
        {
          "data": {
            "as": "Updater",
            "sourceKey": "id",
            "foreignKey": "updatedBy"
          },
          "type": "belongsTo"
        },
        {
          "data": {
            "as": "Destroyer",
            "sourceKey": "id",
            "foreignKey": "deletedBy"
          },
          "type": "belongsTo"
        }
      ],
      "model": "Users"
    },
    {
      "data": [
        {
          "data": {
            "sourceKey": "id",
            "foreignKey": "personId"
          },
          "type": "belongsTo"
        }
      ],
      "model": "Persons"
    }
  ]
}