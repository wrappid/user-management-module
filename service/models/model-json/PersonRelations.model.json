{
  "table": "PersonRelations",
  "database": "application",
  "attributes": {
    "id": {
      "type": "INTEGER",
      "primaryKey": true,
      "autoIncrement": true
    },
    "_status": {
      "type": "STRING"
    },
    "isActive": {
      "type": "BOOLEAN",
      "defaultValue": true
    },
    "deletedAt": {
      "type": "DATE",
      "allowNull": true
    },
    "extraInfo": {
      "type": "JSONB",
      "defaultValue": null
    }
  },
  "associations": [
    {
      "data": [
        {
          "data": {
            "as": "Owner",
            "sourceKey": "id",
            "foreignKey": "createdBy"
          },
          "type": "belongsTo"
        },
        {
          "data": {
            "as": "Updater",
            "sourceKey": "id",
            "foreignKey": "updatedBy"
          },
          "type": "belongsTo"
        },
        {
          "data": {
            "as": "Destroyer",
            "sourceKey": "id",
            "foreignKey": "deletedBy"
          },
          "type": "belongsTo"
        }
      ],
      "model": "Users"
    },
    {
      "data": [
        {
          "data": {
            "as": "Person",
            "sourceKey": "id",
            "foreignKey": "personId"
          },
          "type": "belongsTo"
        },
        {
          "data": {
            "as": "RelatedPerson",
            "sourceKey": "id",
            "foreignKey": "relatedPersonId"
          },
          "type": "belongsTo"
        }
      ],
      "model": "Persons"
    },
    {
      "data": [
        {
          "data": {
            "sourceKey": "id",
            "foreignKey": "relationId"
          },
          "type": "belongsTo"
        }
      ],
      "model": "Relations"
    }
  ]
}